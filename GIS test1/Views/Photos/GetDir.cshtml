
@{
    ViewBag.Title = "GetPhotos";
}


    <div class="row">
        <div class="col-md-offset-3 col-md-6 col-xs-12">
            <div class="well">
                <table class="table table-bordered table-striped table-hover">
                    <thead>
                        <tr>
                            <td>
                                ID
                            </td>
                            <td>
                                Action
                            </td>
                        </tr>
                    </thead>
                    @{
                        foreach (var dir in ViewBag.Dirs)
                        {
                            <tr>
                                <td>
                                    @dir.Name
                                </td>
                                <td>
                                    <button onclick="location.href = '@Url.Action("GetPhotos", "Photos", new { id = @dir.Name })'" type="button" class="btn btn-default btn-sm">OPEN</button>
                                </td>
                            </tr>
                        }

                    }
                </table>
            </div>
        </div>
    </div>


